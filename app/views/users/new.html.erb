<h1>New User</h1>
<div class="row">
	<div class="span6 offset3">
	<%= form_for(@user) do |f| %>
		<%= render 'shared/error_messages', :object => f.object %>

			<%= render 'form',f: f %>
			<%= f.submit "Create my account", class: "btn btn-large btn-primary" %>

		<%= link_to 'Back', users_path %>

		<% end %>
	</div>
</div>
